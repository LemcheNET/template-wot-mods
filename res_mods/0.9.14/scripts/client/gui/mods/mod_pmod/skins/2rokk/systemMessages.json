"systemMessages": {
	
	"battleResults": {
		
		// Сообщение при победе. 
		"winPattern": {
			"message": "{{win}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
			"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_win.png",
			"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png",
			"noBattleData": {
				"message": "{{win}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits1}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
				"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_win.png",
				"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png"
			}
		},
		
		// Сообщение при поражении.
		"defeatPattern": {
			"message": "{{defeat}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
			"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_def.png",
			"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png",
			"noBattleData": {
				"message": "{{defeat}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits1}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
				"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_def.png",
				"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png"
			}
		},
		
		// Сообщение при ничьей.
		"drawPattern": {
			"message": "{{draw}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
			"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_draw.png",
			"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png",
			"noBattleData": {
				"message": "{{draw}}{{vehicle}}{{xp}}{{DailyXPFactor}}{{credits1}}{{CreditsCompensation}}{{CreditsPenalty}}{{Achievements}}",
				"bgImage": "../../scripts/client/gui/mods/mod_pmod/skins/2rokk/images/bg/statBg_draw.png",
				"icon": "../../gui/maps/icons/library/BattleResultIcon-1.png"
			}
		},
		
		// Паттерны макросов.
		"macrosFormatter": {
			"dailyXPFactor": "<font size='12' color='#ff9900' face='Enigmatic Unicode'>  x{{val}}</font>",
			"creditsPenalty": "<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/peni.png' width='16' height='16' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>Штраф:</font> <font size='12' color='#cc0000' face='Enigmatic Unicode'>\t{{val}}</font>\n",
			"creditsCompensation": "<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/comp.png' width='16' height='16' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>Компенс.:</font> <font size='12' color='#00b414' face='Enigmatic Unicode'>\t{{val}}</font>\n",
			"achievements": {
				"pattern": "{{localName}}",
				"result": "<font color='#ce7900' size='12' face='Enigmatic Unicode'><img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/win.png' width='16' height='16' align='baseline' vspace='-4'> {{pattern}}</font>",
				"separator": "\n<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/win.png' width='16' height='16' align='baseline' vspace='-4'> "
			}
		},
		
		"buttons": {
			"label": "Подробнее",
			"type": "black",
			"width": 122
		},
		
		${"mod_pmod/skins/2rokk/_colors.json"},
		
		// Пользовательские макросы
		"userMacros": {
			"win": "<textformat tabstops='[62]' leading='1'><font color='#00b414' size='14' face='Enigmatic Unicode'>{{Map}}</font>\n",
			"defeat": "<textformat tabstops='[62]' leading='1'><font color='#FE0E00' size='14' face='Enigmatic Unicode'>{{Map}}</font>\n",
			"draw": "<textformat tabstops='[62]' leading='1'><font color='#F8F400' size='14' face='Enigmatic Unicode'>{{Map}}</font>\n",
			"vehicle": "<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/tank.png' width='28' height='22' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>{{VehicleLocalName}}</font>\n",
			"xp": "<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/exp.png' width='16' height='16' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>Опыт: </font>\t<font color='{{c:XP}}' face='Enigmatic Unicode' font size='12'>{{XP}}</font>",
			"credits": "\n<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/cred.png' width='16' height='16' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>Кредиты: </font>\t<font color='{{c:PureCredits}}' face='Enigmatic Unicode' font size='12'>{{PureCredits}}</font>\n",
			"credits1": "\n<img src='img://scripts/client/gui/mods/mod_pmod/skins/2rokk/images/cred.png' width='16' height='16' align='baseline' vspace='-4'> <font size='12' color='#FFCC66' face='Enigmatic Unicode'>Кредиты: </font>\t<font color='{{c:Credits}}' face='Enigmatic Unicode' font size='12'>{{Credits}}</font>\n"
		}
	}
}