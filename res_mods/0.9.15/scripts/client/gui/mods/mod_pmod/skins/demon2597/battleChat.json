// Игровой чат в бою.
// Ingame chat in battle.
"battleChat": {
		
	// Информер результатов о завершенном бое в чат.
	// Informer results of completed battle to chat.
	"resultInformer": {
			
		// Текстовые сообщения в чат. Макросы смотрите в README.txt
		// Text messages in chat. Macroses in README.txt
		"resultWin": "<textformat tabstops='[25]'><img src='img://gui/maps/icons/library/BattleResultIcon-1.png' align='baseline' vspace='-2'>  <font size='16'><font color='#00CC00'>ПОБЕДА!</font>\n<tab><font color='#FFCC66'>{{VehicleLocalNameShort}}</font> <font color='#D6D7D4'>({{Map}})</font></font></textformat>",
		"resultDraw": "<textformat tabstops='[25]'><img src='img://gui/maps/icons/library/BattleResultIcon-1.png' align='baseline' vspace='-2'>  <font size='16'><font color='#FFAA00'>НИЧЬЯ</font>\n<tab><font color='#FFCC66'>{{VehicleLocalNameShort}}</font> <font color='#D6D7D4'>({{Map}})</font></font></textformat>",
		"resultDefeat": "<textformat tabstops='[25]'><img src='img://gui/maps/icons/library/BattleResultIcon-1.png' align='baseline' vspace='-2'>  <font size='16'><font color='#F01818'>ПОРАЖЕНИЕ</font>\n<tab><font color='#FFCC66'>{{VehicleLocalNameShort}}</font> <font color='#D6D7D4'>({{Map}})</font></font></textformat>"
	}
}