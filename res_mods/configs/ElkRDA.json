		"ARMOR_PIERCING_HE": "APHE",
		"HIGH_EXPLOSIVE": "HE",
		"HOLLOW_CHARGE": "HEAT"
	},
	"shelltype_color": {
		"ARMOR_PIERCING": "#FFBF00",
		"ARMOR_PIERCING_CR": "#FFBF00",
		"ARMOR_PIERCING_HE": "#FFBF00",
		"HIGH_EXPLOSIVE": "#FFBF00",
		"HOLLOW_CHARGE": "#FFBF00"
	},
	"tankclass_name": {
		"lightTank": "LT",
		"mediumTank": "MT",
		"heavyTank": "HT",
		"SPG": "SPG",
		"AT-SPG": "TD"
	},
	"tankclass_color": {
		"lightTank": "#A2FF9A",
		"mediumTank": "#FFF198",
		"heavyTank": "#FFACAC",
